<div class="container">
   <div class="row my-5">
     <h4>会員一覧</h4>
   </div>
    <div class="row mx-auto">
     
      <table class="table">
        <thead>
          <tr>
            <th>会員ID</th>
            <th >氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>

        <tbody>
          <% @cust.each do |item| %>
            <tr>
              <td> <%= item.id %></td>
              <td><%= link_to "#{item.name}" , admin_item_path(item.id)%></td>
              <td><%= item.price %></td>
              <td><%= item.genre.name %></td>
              <td><%= %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <%= paginate @items %>
   </div>
 </div>
